view.search {
    display: flex;
    flex-direction: row;
    height: 140rpx;
    margin-top: -120rpx;
    width: 100%;
    background: rgb(255, 255, 255);
    align-items: center;
    position: fixed;
    z-index: 99;
}
 
view.searchItem {
    display: flex;
    width: 94%;
    height: 80rpx;
    background: rgba(244,244,244,1);
    flex-direction: row;
    border-radius: 10rpx;
    padding-top: 15rpx;
    margin-right: 30rpx;
    margin-left: 20rpx;
    align-self: center;
    position: fixed;
    z-index: 99;
}
 
view.searchItem image {
    height: 35rpx;
    width: 35rpx;
    align-self: center;
    margin-left: 20rpx;
    margin-right: 20rpx;
    margin-bottom: 20rpx;
}
view.searchItem button {
    height: 50rpx;
    color: #147CEC;
    font-size: 50rpx;
    margin-left: 20rpx;
    margin-right: 20rpx;
    margin-top: 5rpx;
}
 
view.searchItem input {
    flex: 1;
    font-size: 30rpx;
}

