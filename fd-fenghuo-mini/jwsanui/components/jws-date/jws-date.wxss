.jws-date-box {
  position: relative;
}

.jws-date-box .jws-date-box-gray {
  color: rgba(204, 204, 204, 1);
}

.jws-date-box-picker {
  position: fixed;
  top: 0;
  left: 0;
  z-index: 99999999;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.5);
}

.jws-date-box-picker .jws-date-picker {
  background: rgba(255, 255, 255, 1);
  border-radius: 10rpx 10rpx 0px 0px;
  width: 100%;
}
.jws-date-picker-list-box-show{
  min-width: 750rpx;
  width: 750rpx;
  max-width: 750rpx;
}
.jws-date-picker-list-box {
  min-width: 2250rpx;
  width: 2250rpx;
  max-width: 2250rpx;
  position: relative;
}

.jws-date-picker-list {
  min-width: 750rpx;
  width: 750rpx;
  max-width: 750rpx;
  padding: 0rpx 25rpx 25rpx 25rpx;
}

.jws-date-picker-list .jws-date-picker-list-row {
  width: 100%;
  padding: 5rpx 0;
}

.jws-date-picker-list  .jws-date-picker-list-row .jws-date-picker-list-item {
  border-radius: 50%;
  width: 70rpx;
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  display: block;
  font-size: 28rpx;
  border: 1rpx solid rgba(204, 204, 204, 1);
}

.jws-date-picker-list  .jws-date-picker-list-row .jws-date-picker-list-item-selected{
  border: 1rpx solid rgba(204, 204, 204, 1);
  background: var(--cyan);
}


.jws-date-picker-list  .jws-date-picker-list-row .jws-gray{
  color: rgba(26, 26, 26, 0.3);
  background: rgba(204, 204, 204, 0.3);
  border: 1rpx solid rgba(204, 204, 204, 0.3);
}

.jws-date-picker-week-box {
  width: 100%;
  padding: 0 25rpx;
}

.jws-date-picker-week-box .jws-date-picker-week {
  width: 70rpx;
  padding: 20rpx;
  text-align: center;
  font-size: 33rpx;
}

.jws-date-picker-list  .jws-date-picker-list-row .jws-date-picker-list-item:active {
  color: rgba(0, 0, 0, 0.6) !important;
  background-color: #dedede !important;
}

.jws-click:active {
  color: rgba(0, 0, 0, 0.6) !important;
  background-color: #dedede !important;
}

.jws-date-picker-list-option {
  width: 100%;
  padding: 20rpx 25rpx 0 25rpx;
}

.jws-date-picker-list-option .jws-date-picker-list-row-elect {
  display: inline-flex;
  padding-left: 20rpx;
}

