.jws-date-picker-list-option .jws-date-picker-list-row-elect .jws-elect-txt {
  font-size: 35rpx;
  color-adjust: rgba(51, 51, 51, 1);
}

.jws-date-picker-list-option .jws-date-picker-list-row-confirm {
  display: inline-flex;
}

.jws-mgl20 {
  margin-left: 20rpx;
}

.jws-mgl30 {
  margin-left: 30rpx;
}

.jws-swiper {
  height: 400rpx;
}

/* 动画内容在这里添加 */

.jws-show-box {
  animation: jws-show 0.4s;
}

.jws-show-picker {
  animation: jws-slide-top 0.4s;
}

.jws-hide-box {
  animation: jws-hide 0.4s;
}

.jws-hide-picker {
  animation: jws-slide-bottom 0.4s;
}
