.jws-upload-img {
  position: relative;
  width: 100%;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
}

.jws-upload-row {
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  box-sizing: border-box;
}

.jws-upload-img-item {
  width: 140rpx;
  height: 140rpx;
  position: relative;
  display: flex;
  border-radius: 10rpx;
  margin-right: 20rpx;
}

.jws-upload-img-item image {
  display: inline-block;
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
}

.jws-upload-img-item:nth-child(n+5) {
  margin-top: 20rpx;
}

.jws-upload-img-item .jws-upload-img-del {
  width: 40rpx;
  height: 40rpx;
  background: rgba(255, 76, 76, 1);
  border: 2rpx solid rgba(255, 255, 255, 1);
  position: absolute;
  top: -12rpx;
  right: -12rpx;
  z-index: 9;
  border-radius: 50%;
}

.jws-upload-img-item .jws-upload-img-del .del-icon1 {
  width: 25rpx;
  height: 6rpx;
  background: rgba(255, 255, 255, 1);
  -webkit-transform: rotate(45deg);
  position: absolute;
  z-index: 8;
  top: 40%;
  left: 18%;
}

.jws-upload-img-item .jws-upload-img-del .del-icon2 {
  width: 25rpx;
  height: 6rpx;
  background: rgba(255, 255, 255, 1);
  -webkit-transform: rotate(-45deg);
  position: absolute;
  z-index: 9;
  top: 40%;
  left: 18%;
}

.jws-upload-row .jws-upload-txt {
  font-size: 24rpx;
  color: rgba(255, 76, 76, 1);
}

.mgt5 {
  margin-top: 5rpx;
}

.jws-add-icon {
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  /* border: 2rpx dashed rgba(204, 204, 204, 1); */
  background: url("data:image/svg+xml;base64,");
  background-repeat: no-repeat;
  background-size: 100%;
}

.jws-add-icon .add-icon-additionx {
  position: relative;
  width: 50rpx;
  height: 4rpx;
  background: rgba(204, 204, 204, 1);
}

.jws-add-icon .add-icon-additionx .add-icon-additiony {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  width: 4rpx;
  height: 50rpx;
  background: rgba(204, 204, 204, 1);
}

.jws-add-icon .add-icon-camera {
  display: inline-block;
  width: 60rpx;
  height: 45rpx;
  background: url("data:image/svg+xml;base64,");
  background-repeat: no-repeat;
  background-position: 50%;
  background-size: 100%;
  transform-origin: 50%;
}
